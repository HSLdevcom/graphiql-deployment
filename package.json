    "node": ">=16.17",
    "npm": ">=3.0"
  },
  "packageManager": "yarn@4.5.1",
  "private": true,
  "homepage": "/graphiql",
  "dependencies": {
    "graphiql": "^3.7.1",
    "graphql": "^16.9.0",
    "react": "^18.3.1",
